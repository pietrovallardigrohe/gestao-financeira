{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug gestao-financeira (Native Debug)",
      "type": "gdb",
      "request": "launch",
      "target": "${workspaceFolder}/debug/gestao-financeira.exe",
      "cwd": "${workspaceFolder}/debug",
      "valuesFormatting": "parseText",
      "gdbpath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "build debug",
      "env": {
        "QT_OPENGL": "software",
        "QT_QUICK_BACKEND": "software"
      }
    },
    {
      "name": "Debug gestao-financeira (C/C++ Extension)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/debug/gestao-financeira.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/debug",
      "environment": [
        {
          "name": "QT_OPENGL",
          "value": "software"
        },
        {
          "name": "QT_QUICK_BACKEND",
          "value": "software"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "build debug"
    },
    {
      "name": "Debug with CodeLLDB",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/debug/gestao-financeira.exe",
      "args": [],
      "cwd": "${workspaceFolder}/debug",
      "stopOnEntry": false,
      "environment": [
        {
          "name": "QT_OPENGL",
          "value": "software"
        },
        {
          "name": "QT_QUICK_BACKEND",
          "value": "software"
        },
        {
          "name": "QT_DEBUG_PLUGINS",
          "value": "1"
        }
      ],
      "externalConsole": false,
      "preLaunchTask": "build debug"
    }
  ]
}
